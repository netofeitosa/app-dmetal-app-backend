// mysql-schema.prisma

generator client {
  provider      = "prisma-client-js"
  output        = "./generated/mysql"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_MYSQL_URL")
}

model users {
  id        Int     @id @default(autoincrement())
  login     Int     @unique
  usuario   String  @unique
  nome      String
  image_url String?
  senha     String
  token     String  @default("")
}
